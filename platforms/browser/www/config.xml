<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ssausm.contactcard" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Contact Card</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SSAUSM CC</name>
    <description>
        SSAUSM (NJ, USA) - Contact Card Application: To collect contact information.
    </description>
    <author email="ssausm-nj-usa@pi9.eu" href="http://www.svg.org">
        SSAUSM IT Team
    </author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-navigation href="https://goo.gl/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
